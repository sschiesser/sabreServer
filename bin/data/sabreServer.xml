    <!-- comment this to change to receiver-->
    <!-- <serialport>/dev/tty.usbserial-A600dJ1Z</serialport> -->
    <baudrate>230400</baudrate>
    <framerate>10</framerate>
    <!-- display framerate -->
    <OSCsender>
        <interval>4</interval>
        <!-- OSC sending interval -->
        <sendRawValues>1</sendRawValues>
        <!-- include raw values in OSC stream -->
    </OSCsender>
    <network>
        <sender id="1">
            <IP>localhost</IP>
            <port>40002</port>
            <active>1</active>
            <!-- activate this OSC sender -->
        </sender>
        <sender id="2">
            <IP>127.0.0.1</IP>
            <port>57120</port>
            <active>0</active>
            <mode>16</mode>
            <!-- change the OSC sending mode -->
        </sender>
        <sender id="3">
            <IP>127.0.0.1</IP>
            <port>40003</port>
            <active>0</active>
        </sender>
        <sender id="4">
            <IP>127.0.0.1</IP>
            <port>40004</port>
            <active>0</active>
        </sender>
        <receiver>
            <port>41003</port>
        </receiver>
    </network>
    <thresholds>
        <down>0.45</down>
        <up>0.55</up>
    </thresholds>
    <debounce-timeout>0</debounce-timeout>
    <accel-resolution>4</accel-resolution>
    <!-- 2g/4g/8g/16g 10/11/12/13bit resolution -->
